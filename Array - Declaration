#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int main() {

	int A[5] = {2,4,6,8,10};       //Static Arrays
	int B[5] = { 1,2,3,4,5 };
	int C[10] = { 2,4,6, };
	int D[5] = { 0 };
	int E[] = { 1,2,3,4,5,6, };

	for (int i = 0; i < 5; i++) {
		cout << A[i] << " ";
	}

	int* p;                         //Dynamic Array
	p = new int[5];
	p[0] = 1;
	p[1] = 3;
	p[2] = 5;
	p[3] = 7;
	p[4] = 9;

	for (int i = 0; i < 5; i++) {
		cout << p[i]<<" ";
	}
}
