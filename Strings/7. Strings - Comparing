#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int main() {
		
	char A[] = "painter";
	char B[] = "painter";
	
	int i;
	int j;
	
	for(i=0,j=0; A[i]!='\0' && B[j]!='\0'; i++,j++){
		if(A[i] != B[j]){
			cout<<"The strings are not equal and ";
			break;
		}
	}
	
	if(A[i] == B[j]){
		cout<<"The strings are equal.";
	}
	else if(A[i] < B[j]){
		cout<<"A is smaller.";
	}
	else{
		cout<<"A is greater.";
	}

	return 0;
}
