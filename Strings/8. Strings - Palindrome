#include <iostream>
#include <stdio.h>
#include <stdlib.h>

using namespace std;

int main() {
		
	char A[] = "racecae";
	char B[8] ;
	int i=0;
	int j;
	
	for(j=0; A[j]!='\0'; j++){
	}
	j=j-1;
	
	while(i<j){
		if(A[i] == A[j]){
			i++;
			j--;
		}
		else{
			cout<<"The string is not a palindrome.";
			break;
		}
	}
	if(A[i] == A[j]){
		cout<<"The string is a palindrome.";
	}
	
	return 0;
}
